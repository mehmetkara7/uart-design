T_0.2 ;
    %load/vec4 v0x55dc0abef6c0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.3, 5;
    %wait E_0x55dc0abc7e30;
    %load/vec4 v0x55dc0abef3c0_0;
    %load/vec4 v0x55dc0abef6c0_0;
